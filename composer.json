{
    "name": "media/unitconversation",
    "description": "unitconversation",
    "keywords": [
        "media",
        "unitconversation"
    ],
    "homepage": "https://github.com/media/unitconversation",
    "license": "MIT",
    "authors": [
        {
            "name": "damir",
            "email": "dt@23media.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Media\\Unitconversation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Media\\Unitconversation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
